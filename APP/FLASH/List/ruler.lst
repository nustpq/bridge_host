   1380          unsigned int Revert_Bit_Order(unsigned int data,unsigned char bit_len )
   1381          {
   1382              signed char i;
   1383              unsigned int Temp1 = 0;
   1384              unsigned int Temp2 = 0;
   1385              
   1386              for( i = bit_len - 1; i >= 0; i-- )  {
   1387                  Temp1 = data & 0x01;
   1388                  Temp1 <<= i;
   1389                  Temp2 |= Temp1;
   1390                  data >>= 1;
   1391              }
   1392              
   1393              return Temp2;
   1394              
   1395          }
