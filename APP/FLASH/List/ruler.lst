   1103          * Description : Add code area protection for FLASHD_Write()
   1104          *               Writes a data buffer in the internal flash. This function works in polling
   1105          *               mode, and thus only returns when the data has been effectively written.    
   1106          * Argument(s) :  address  Write address.
   1107          *                pBuffer  Data buffer.
   1108          *                size     Size of data buffer in bytes.
   1109          * Return(s)   : NO_ERR :   execute successfully
   1110          *               others :   =error code .  
   1111          *
   1112          * Note(s)     : None.
   1113          *********************************************************************************************************
   1114          */
   1115          unsigned char FLASHD_Write_Safe( unsigned int address, const void *pBuffer,  unsigned int size)
   1116          {
   1117              unsigned char err;
   1118              if( address < AT91C_IFLASH + AT91C_IFLASH_CODE_SIZE ) {
   1119                  APP_TRACE_INFO(("ERROR: this operation wanna flush code area!\r\n"));  
   1120                  return FW_BIN_SAVE_ADDR_ERR;
   1121              }
   1122              err = FLASHD_Write(  address, pBuffer, size );
   1123              return err;  
   1124              
   1125          }
