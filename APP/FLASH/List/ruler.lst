   1263          /*
   1264          *********************************************************************************************************
   1265          *                                       Update_Ruler_FW()
   1266          *
   1267          * Description :  Write firmware to specified ruler's MCU flash
   1268          *               
   1269          * Argument(s) :  ruler_slot_id :  0~ 3.     
   1270          *
   1271          * Return(s)   : NO_ERR :   execute successfully
   1272          *               others :   =error code .  
   1273          *
   1274          * Note(s)     : Do not care if ruler is attached or not.Because host can not detect ruler if FW was crashed.
   1275          *********************************************************************************************************
   1276          */
   1277          unsigned char Update_Ruler_FW( unsigned char ruler_slot_id )
   1278          {
   1279              unsigned char err;
   1280              unsigned int  flash_addr; 
   1281              FLASH_INFO   *pFlash_Info;
   1282              unsigned char Buf[9];
   1283              unsigned char i;
   1284              
   1285              err = NO_ERR;
   1286              flash_addr  = FLASH_ADDR_FW_BIN;
   1287              pFlash_Info = (FLASH_INFO *)FLASH_ADDR_FW_STATE ;
   1288                  
   1289              if( pFlash_Info->f_w_state != FW_DOWNLAD_STATE_FINISHED ) {
   1290                  APP_TRACE_INFO(("ERROR: FW bin file missed!\r\n"));        
   1291                  return FW_BIN_STATE_ERR;
   1292              }
   1293              
   1294              APP_TRACE_INFO(("Start updating ruler[%d] firmware to \"%s\" version ...\r\n",ruler_slot_id,pFlash_Info->bin_name)); 
   1295              memset(Buf,'d',sizeof(Buf)); //send 'd' to start download  
   1296              Ruler_Power_Switch(0);   //power off ruler  
   1297              OSTimeDly(200);   
   1298              for( i = 0; i < 4; i++ ) {
   1299                  Global_Ruler_State[i] = RULER_STATE_DETACHED ;
   1300              }
   1301              UART_Init(RULER_UART,  NULL,  115200 );   //Init Ruler to inquire mode
   1302              Port_Detect_Enable(0); //disable ruler detect
   1303              
   1304              OSSemPend( UART_MUX_Sem_lock, 0, &err ); 
   1305              if( Global_Ruler_Index != ruler_slot_id ) {
   1306                  Check_UART_Mixer_Ready();
   1307                  //OS_ENTER_CRITICAL(); 
   1308                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
   1309                  //OS_EXIT_CRITICAL();  
   1310                  UART1_Mixer( ruler_slot_id );
   1311              }
   1312              
   1313              Ruler_Power_Switch(1);   //power on ruler
   1314              OSTimeDly(200);
   1315              err = USART_SendBuf( RULER_UART, Buf,  sizeof(Buf));
   1316              if( OS_ERR_NONE == err ) {     
   1317                  OSTimeDly(800);
   1318                  err = USART_Read_Timeout( RULER_UART, Buf, 3, 5000 );
   1319                  if( OS_ERR_NONE == err && ( Buf[0] == 'c' || Buf[0] == 'C' )) {
   1320                      Global_Ruler_State[ruler_slot_id] = RULER_STATE_RUN ;
   1321                      err = Xmodem_Transmit( (unsigned char *)flash_addr, pFlash_Info->bin_size );
   1322                      Global_Ruler_State[ruler_slot_id] = RULER_STATE_DETACHED ;            
   1323                  }         
   1324              }
   1325              if( OS_ERR_NONE != err ) {
   1326                  APP_TRACE_INFO(("\r\nFailed to init ruler bootloader. Err Code = [0x%X]\r\n", err));        
   1327              } else {
   1328                  APP_TRACE_INFO(("\r\nUpdate ruler[%d] firmware successfully!\r\n", ruler_slot_id));   
   1329              }
   1330              Port_Detect_Enable(1); //enable ruler detect
   1331              UART_Init(RULER_UART,  ISR_Ruler_UART,  115200 );  //Init Ruler back to interuption mode
   1332              Ruler_Power_Switch(0);   //power off ruler  
   1333              OSTimeDly(500);    
   1334              Ruler_Power_Switch(1);   //power on ruler
   1335              OSSemPost( UART_MUX_Sem_lock ); 
   1336              return err ;    
   1337              
   1338          }
