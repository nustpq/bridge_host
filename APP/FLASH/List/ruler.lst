   1250          /*
   1251          *********************************************************************************************************
   1252          *                                       Update_Ruler_FW()
   1253          *
   1254          * Description :  Write firmware to specified ruler's MCU flash
   1255          *               
   1256          * Argument(s) :  ruler_slot_id :  0~ 3.     
   1257          *
   1258          * Return(s)   : NO_ERR :   execute successfully
   1259          *               others :   =error code .  
   1260          *
   1261          * Note(s)     : Do not care if ruler is attached or not.Because host can not detect ruler if FW was crashed.
   1262          *********************************************************************************************************
   1263          */
   1264          unsigned char Update_Ruler_FW( unsigned char ruler_slot_id )
   1265          {
   1266              unsigned char err;
   1267              unsigned int  flash_addr; 
   1268              FLASH_INFO   *pFlash_Info;
   1269              unsigned char Buf[9];
   1270              unsigned char i;
   1271              
   1272              err = NO_ERR;
   1273              flash_addr  = FLASH_ADDR_FW_BIN;
   1274              pFlash_Info = (FLASH_INFO *)FLASH_ADDR_FW_STATE ;
   1275                  
   1276              if( pFlash_Info->f_w_state != FW_DOWNLAD_STATE_FINISHED ) {
   1277                  APP_TRACE_INFO(("ERROR: FW bin file missed!\r\n"));        
   1278                  return FW_BIN_STATE_ERR;
   1279              }
   1280              
   1281              APP_TRACE_INFO(("Start updating ruler[%d] firmware to \"%s\" version ...\r\n",ruler_slot_id,pFlash_Info->bin_name)); 
   1282              memset(Buf,'d',sizeof(Buf)); //send 'd' to start download  
   1283              Ruler_Power_Switch(0);   //power off ruler  
   1284              OSTimeDly(200);   
   1285              for( i = 0; i < 4; i++ ) {
   1286                  Global_Ruler_State[i] = RULER_STATE_DETACHED ;
   1287              }
   1288              UART_Init(RULER_UART,  NULL,  115200 );   //Init Ruler to inquire mode
   1289              Port_Detect_Enable(0); //disable ruler detect
   1290              
   1291              OSSemPend( UART_MUX_Sem_lock, 0, &err ); 
   1292              if( Global_Ruler_Index != ruler_slot_id ) {
   1293                  Check_UART_Mixer_Ready();
   1294                  //OS_ENTER_CRITICAL(); 
   1295                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
   1296                  //OS_EXIT_CRITICAL();  
   1297                  UART1_Mixer( ruler_slot_id );
   1298              }
   1299              
   1300              Ruler_Power_Switch(1);   //power on ruler
   1301              OSTimeDly(200);
   1302              err = USART_SendBuf( RULER_UART, Buf,  sizeof(Buf));
   1303              if( OS_ERR_NONE == err ) {     
   1304                  OSTimeDly(800);
   1305                  err = USART_Read_Timeout( RULER_UART, Buf, 3, 5000 );
   1306                  if( OS_ERR_NONE == err && ( Buf[0] == 'c' || Buf[0] == 'C' )) {
   1307                      Global_Ruler_State[ruler_slot_id] = RULER_STATE_RUN ;
   1308                      err = Xmodem_Transmit( (unsigned char *)flash_addr, pFlash_Info->bin_size );
   1309                      Global_Ruler_State[ruler_slot_id] = RULER_STATE_DETACHED ;            
   1310                  }         
   1311              }
   1312              if( OS_ERR_NONE != err ) {
   1313                  APP_TRACE_INFO(("\r\nFailed to init ruler bootloader. Err Code = [0x%X]\r\n", err));        
   1314              } else {
   1315                  APP_TRACE_INFO(("\r\nUpdate ruler[%d] firmware successfully!\r\n", ruler_slot_id));   
   1316              }
   1317              Port_Detect_Enable(1); //enable ruler detect
   1318              UART_Init(RULER_UART,  ISR_Ruler_UART,  115200 );  //Init Ruler back to interuption mode
   1319              Ruler_Power_Switch(0);   //power off ruler  
   1320              OSTimeDly(500);    
   1321              Ruler_Power_Switch(1);   //power on ruler
   1322              OSSemPost( UART_MUX_Sem_lock ); 
   1323              return err ;    
   1324              
   1325          }
