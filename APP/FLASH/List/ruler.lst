    696                  temp = data_length > (NOAH_CMD_DATA_MLEN-1) ? (NOAH_CMD_DATA_MLEN-1) : data_length ;  
    697                  err = pcSendDateToBuf( EVENT_MsgQ_Noah2RulerUART, FRAM_TYPE_DATA, pdata, temp, 0, buf, 1 ) ; 
    698                  if( OS_ERR_NONE != err ) { break;}
    699                  OSTimeDly(50); //wait for ruler operation
    700                  data_length -= temp;
    701                  pdata += temp;
    702              }
