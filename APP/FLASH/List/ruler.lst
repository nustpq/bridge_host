    673              buf[0] =  RULER_CMD_WRITE_RULER_INFO;
    674              buf[1] =  EMB_DATA_FRAME;  
    675              buf[2] = (pEBuf_Cmd->length) & 0xFF;    
    676              buf[3] = ((pEBuf_Cmd->length)>>8) & 0xFF;     
