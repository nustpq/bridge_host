   1529          /*
   1530          *********************************************************************************************************
   1531          *                                       Ruler_Port_LED_Service()
   1532          *
   1533          * Description : Control the ruler port identify LED state:  
   1534          *               turn on LED after ruler configured, blink LED during recording
   1535          * Argument(s) : None.
   1536          * Return(s)   : None.
   1537          * Note(s)     : None.
   1538          *********************************************************************************************************
   1539          */
   1540          void Ruler_Port_LED_Service( void )
   1541          {    
   1542              static unsigned int counter; 
   1543              static unsigned int counter_buz;    
   1544              unsigned char ruler_id;
   1545              unsigned char ruler_state;    
   1546              unsigned char LED_Freq;
   1547              unsigned char post_err_flag;
   1548          
   1549              LED_Freq      = 0x3F; 
   1550              post_err_flag = 0;
   1551              
   1552              for( ruler_id = 0 ; ruler_id < 4 ; ruler_id++ ) {
   1553                
   1554                  ruler_state = Global_Ruler_State[ruler_id];  
   1555                  if( Global_Bridge_POST != NO_ERR ) { //if POST err, start all LED 
   1556                      ruler_state = RULER_STATE_RUN ;
   1557                      post_err_flag = 1;
   1558                  }
   1559                  switch( ruler_state ) {
   1560                    
   1561                      case RULER_STATE_DETACHED :
   1562                      case RULER_STATE_ATTACHED :
   1563                          LED_Clear( LED_P0 + ruler_id );
   1564                      break;            
   1565                      case RULER_STATE_CONFIGURED :
   1566                      case RULER_STATE_SELECTED :  
   1567                          LED_Set( LED_P0 + ruler_id );
   1568                      break;            
   1569                      case RULER_STATE_RUN :
   1570                          if( (counter & LED_Freq) == 0 ) {
   1571                              LED_Toggle( LED_P0 + ruler_id );   
   1572                              if( post_err_flag== 1 && ruler_id == 0 && (counter_buz++ < 6 ) ) {
   1573                                  //Buzzer_Toggle(); //buzzer off id POST err 
   1574                                  Buzzer_OnOff( counter_buz&0x01 );   //fix long buz issue in some case                  
   1575                              }
   1576                          }
   1577                      
   1578                      default:              
   1579                      break;
   1580                  }
   1581            
   1582              }    
   1583              counter++;    
   1584          }
