   1373          /*
   1374          *********************************************************************************************************
   1375          *                                       Toggle_Mic()
   1376          *
   1377          * Description : Toggle specified mic's active state by sending command to related ruler and updating 
   1378          *               FPGA mic signal switch array. 
   1379          *               One mic One time.
   1380          * Argument(s) : pdata : pointer to TOGGLE_MIC structure data
   1381          * Return(s)   : NO_ERR :   execute successfully
   1382          *               others :   =error code .  
   1383          *
   1384          * Note(s)     : None.
   1385          *********************************************************************************************************
   1386          */
   1387          unsigned char Toggle_Mic(  TOGGLE_MIC *pdata )
   1388          {  
   1389              unsigned char  err ;
   1390              unsigned char  id;
   1391              unsigned int   mic_mask;  
   1392              unsigned int   fpga_mask;
   1393              
   1394          #if OS_CRITICAL_METHOD == 3u
   1395              OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
   1396          #endif 
   1397              fpga_mask = 0;
   1398              //check ruler connection state 
   1399              if( Global_Ruler_State[pdata->ruler_id] < RULER_STATE_CONFIGURED ) {      
   1400                  return RULER_STATE_ERR ;         
   1401              }  
   1402              APP_TRACE_INFO(("Toggle Ruler[%d]-Mic[%d] : %d  : ", pdata->ruler_id, pdata->mic_id, pdata->on_off )); 
   1403              OS_ENTER_CRITICAL(); 
   1404              mic_mask = Global_Mic_Mask[pdata->ruler_id];
   1405              OS_EXIT_CRITICAL();  
   1406              mic_mask &= ~( 1<<(pdata->mic_id));
   1407              mic_mask |=  (pdata->on_off&0x01)<<( pdata->mic_id);
   1408              err = Update_Mic_Mask( pdata->ruler_id, mic_mask );
   1409              APP_TRACE_INFO((" %s [0x%X]\r\n", err == OS_ERR_NONE ? "OK" : "FAIL" , err )); 
   1410              if( OS_ERR_NONE != err ) {        
   1411                  return err;    
   1412              }
   1413              OS_ENTER_CRITICAL(); 
   1414              Global_Mic_Mask[pdata->ruler_id] = mic_mask; 
   1415              //APP_TRACE_INFO(("Update Ruler[%d] Mic_Mask:  %d\r\n",pdata->ruler_id,Global_Mic_Mask[pdata->ruler_id]));   
   1416              if( mic_mask == 0 ) {      
   1417                  Global_Ruler_State[pdata->ruler_id] = RULER_STATE_CONFIGURED;         
   1418              } else {
   1419                  Global_Ruler_State[pdata->ruler_id] = RULER_STATE_SELECTED;  
   1420              }
   1421              OS_EXIT_CRITICAL();
   1422              if( RULER_TYPE_MASK( Global_Ruler_Type[pdata->ruler_id] ) == RULER_TYPE_RULER ) { //ruler
   1423                  for( id = 0; id < 4; id++ ) {
   1424                      fpga_mask += (Global_Mic_Mask[id]&0xFF) << (id<<3);
   1425                  }
   1426              } else { //handset
   1427                 fpga_mask = 0x3F << ((pdata->ruler_id)<<3);
   1428              }
   1429              Init_FPGA(fpga_mask);
   1430              return err;  
   1431          }
