    252              err = USART_Read_Timeout( AUDIO_UART, &data, 1, TIMEOUT_AUDIO_COM);  
    253              if( err != NO_ERR ) { 
    254                  APP_TRACE_INFO(("\r\nStart_Audio ERROR: timeout\r\n")); 
