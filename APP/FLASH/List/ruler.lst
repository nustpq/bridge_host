   1572          /*
   1573          *********************************************************************************************************
   1574          *                                       Ruler_Port_LED_Service()
   1575          *
   1576          * Description : Control the ruler port identify LED state:  
   1577          *               turn on LED after ruler configured, blink LED during recording
   1578          * Argument(s) : None.
   1579          * Return(s)   : None.
   1580          * Note(s)     : None.
   1581          *********************************************************************************************************
   1582          */
   1583          void Ruler_Port_LED_Service( void )
   1584          {    
   1585              static unsigned int counter; 
   1586              static unsigned int counter_buz;    
   1587              unsigned char ruler_id;
   1588              unsigned char ruler_state;    
   1589              unsigned char LED_Freq;
   1590              unsigned char post_err_flag;
   1591          
   1592              LED_Freq      = 0x3F; 
   1593              post_err_flag = 0;
   1594              
   1595              for( ruler_id = 0 ; ruler_id < 4 ; ruler_id++ ) {
   1596                
   1597                  ruler_state = Global_Ruler_State[ruler_id];  
   1598                  if( Global_Bridge_POST != NO_ERR ) { //if POST err, start all LED 
   1599                      ruler_state = RULER_STATE_RUN ;
   1600                      post_err_flag = 1;
   1601                  }
   1602                  switch( ruler_state ) {
   1603                    
   1604                      case RULER_STATE_DETACHED :
   1605                      case RULER_STATE_ATTACHED :
   1606                          LED_Clear( LED_P0 + ruler_id );
   1607                      break;            
   1608                      case RULER_STATE_CONFIGURED :
   1609                      case RULER_STATE_SELECTED :  
   1610                          LED_Set( LED_P0 + ruler_id );
   1611                      break;            
   1612                      case RULER_STATE_RUN :
   1613                          if( (counter & LED_Freq) == 0 ) {
   1614                              LED_Toggle( LED_P0 + ruler_id );   
   1615                              if( post_err_flag== 1 && ruler_id == 0 && (counter_buz++ < 6 ) ) {
   1616                                  //Buzzer_Toggle(); //buzzer off id POST err 
   1617                                  Buzzer_OnOff( counter_buz&0x01 );   //fix long buz issue in some case                  
   1618                              }
   1619                          }
   1620                      
   1621                      default:              
   1622                      break;
   1623                  }
   1624            
   1625              }    
   1626              counter++;    
   1627          }
