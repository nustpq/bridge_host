    415          /*
    416          *********************************************************************************************************
    417          *                                       Init_Ruler()
    418          *
    419          * Description : Communicate with ruler to check connected or not
    420          *    
    421          * Argument(s) : ruler_slot_id: 0~ 3.
    422          * Return(s)   : NO_ERR :   ruler connected
    423          *               others :   =error code . ruler connection error,           
    424          *
    425          * Note(s)     : None.
    426          *********************************************************************************************************
    427          */
    428          unsigned char Init_Ruler( unsigned char ruler_slot_id ) //0 ~ 3
    429          {
    430              unsigned char err ;
    431          
    432          #if OS_CRITICAL_METHOD == 3u
    433              //OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
    434          #endif 
    435                
    436              //check ruler connection state 
    437              if( Global_Ruler_State[ruler_slot_id] < RULER_STATE_ATTACHED ) {      
    438                  return RULER_STATE_ERR ;         
    439              } 
    440              OSSemPend( UART_MUX_Sem_lock, 0, &err );
    441              if( Global_Ruler_Index != ruler_slot_id ) {
    442                  Check_UART_Mixer_Ready();
    443                  //OS_ENTER_CRITICAL(); 
    444                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
    445                  //OS_EXIT_CRITICAL();  
    446                  UART1_Mixer( ruler_slot_id );
    447              } 
    448              
    449              err = pcSendDateToBuf( EVENT_MsgQ_Noah2RulerUART, FRAM_TYPE_EST, NULL, 0, 0, NULL, 0 ) ; 
    450              if( OS_ERR_NONE == err ) {
    451                  OSSemPend( Done_Sem_RulerUART, TIMEOUT_RULER_COM, &err );  
    452                  if( OS_ERR_TIMEOUT == err ) {
    453                      APP_TRACE_INFO(("Init ruler[%d] timeout!\r\n",ruler_slot_id));            
    454                  } else {
    455                      err = Ruler_CMD_Result; //exe result from GACK
    456                      if(OS_ERR_NONE != err ){
    457                          APP_TRACE_INFO(("Init_Ruler[%d] err = %d\r\n",ruler_slot_id,err));
    458                      }
    459                  }
    460                  
    461              } else {
    462                  APP_TRACE_INFO(("Ruler[%d] pcSendDateToBuf failed: %d\r\n",ruler_slot_id,err));        
    463              }    
    464              OSSemPost( UART_MUX_Sem_lock );    
    465              return err ;    
    466          }
