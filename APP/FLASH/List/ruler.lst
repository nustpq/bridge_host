    704                  OSSemPend( Done_Sem_RulerUART, TIMEOUT_RULER_COM, &err );  
    705                  if( OS_ERR_TIMEOUT == err ) {
    706                      APP_TRACE_INFO(("Write_Ruler_Info[%d] timeout\r\n",ruler_slot_id));
