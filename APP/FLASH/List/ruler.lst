    710          *
    711          * Note(s)     : None.
    712          *********************************************************************************************************
    713          */
    714          unsigned char Write_Ruler_Info( unsigned char ruler_slot_id )
    715          {
    716              unsigned char   err;
    717              unsigned short  data_length;
    718              unsigned char   temp;
    719              unsigned char  *pdata;
    720              unsigned char   buf[4];  
    721              EMB_BUF        *pEBuf_Cmd;        
