    403          *********************************************************************************************************
    404          *                                       Init_Ruler()
    405          *
    406          * Description : Communicate with ruler to check connected or not
    407          *    
    408          * Argument(s) : ruler_slot_id: 0~ 3.
    409          * Return(s)   : NO_ERR :   ruler connected
    410          *               others :   =error code . ruler connection error,           
    411          *
    412          * Note(s)     : None.
    413          *********************************************************************************************************
    414          */
    415          unsigned char Init_Ruler( unsigned char ruler_slot_id ) //0 ~ 3
    416          {
    417              unsigned char err ;
    418          
    419          #if OS_CRITICAL_METHOD == 3u
    420              //OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
    421          #endif 
    422                
    423              //check ruler connection state 
    424              if( Global_Ruler_State[ruler_slot_id] < RULER_STATE_ATTACHED ) {      
    425                  return RULER_STATE_ERR ;         
    426              } 
    427              OSSemPend( UART_MUX_Sem_lock, 0, &err );
    428              if( Global_Ruler_Index != ruler_slot_id ) {
    429                  Check_UART_Mixer_Ready();
    430                  //OS_ENTER_CRITICAL(); 
    431                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
    432                  //OS_EXIT_CRITICAL();  
    433                  UART1_Mixer( ruler_slot_id );
    434              } 
    435              
    436              err = pcSendDateToBuf( EVENT_MsgQ_Noah2RulerUART, FRAM_TYPE_EST, NULL, 0, 0, NULL, 0 ) ; 
    437              if( OS_ERR_NONE == err ) {
    438                  OSSemPend( Done_Sem_RulerUART, TIMEOUT_RULER_COM, &err );  
    439                  if( OS_ERR_TIMEOUT == err ) {
    440                      APP_TRACE_INFO(("Init ruler[%d] timeout!\r\n",ruler_slot_id));            
    441                  } else {
    442                      err = Ruler_CMD_Result; //exe result from GACK
    443                      if(OS_ERR_NONE != err ){
    444                          APP_TRACE_INFO(("Init_Ruler[%d] err = %d\r\n",ruler_slot_id,err));
    445                      }
    446                  }
    447                  
    448              } else {
    449                  APP_TRACE_INFO(("Ruler[%d] pcSendDateToBuf failed: %d\r\n",ruler_slot_id,err));        
    450              }    
    451              OSSemPost( UART_MUX_Sem_lock );    
    452              return err ;    
    453          }
