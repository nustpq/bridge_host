    228          /*
    229          *********************************************************************************************************
    230          *                                           Start_Audio()
    231          *
    232          * Description : Send command to start USB audio play/record.
    233          * Argument(s) : cmd_type : record£¨== 1£©/play£¨== 2£©/record & play £¨== 3£©
    234          * Return(s)   : NO_ERR :   execute successfully
    235          *               others :   refer to error code defines.           
    236          *
    237          * Note(s)     : None.
    238          *********************************************************************************************************
    239          */
    240          unsigned char Start_Audio( unsigned char cmd_type )
    241          {   
    242              unsigned char err   = 0xFF;  
    243              unsigned char data  = 0xFF; 
    244              unsigned char ruler_id;    
    245              unsigned char buf[] = { CMD_DATA_SYNC1, CMD_DATA_SYNC2, RULER_CMD_START_AUDIO, cmd_type&0x03 }; 
    246              
    247          #if OS_CRITICAL_METHOD == 3u
    248              OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
    249          #endif 
    250              APP_TRACE_INFO(("Start_Audio : type = [%d]\r\n", cmd_type));
    251              UART2_Mixer(3); 
    252              USART_SendBuf( AUDIO_UART, buf,  sizeof(buf)) ;    
    253              err = USART_Read_Timeout( AUDIO_UART, &data, 1, TIMEOUT_AUDIO_COM);  
    254              if( err != NO_ERR ) { 
    255                  APP_TRACE_INFO(("\r\nStart_Audio ERROR: Timeout : %d\r\n",err)); 
    256                  return err;
    257              }
    258              if( data != NO_ERR ) {
    259                  APP_TRACE_INFO(("\r\nStart_Audio ERROR: Data : %d\r\n ",data)); 
    260                  return data; 
    261              } else {
    262                  OS_ENTER_CRITICAL(); 
    263                  for( ruler_id = 0 ; ruler_id < 4 ; ruler_id++ ) {       
    264                      if( Global_Ruler_State[ruler_id] ==  RULER_STATE_SELECTED ) {//given: if mic selected, then ruler used
    265                          Global_Ruler_State[ruler_id] = RULER_STATE_RUN;                 
    266                      }      
    267                  }
    268                  OS_EXIT_CRITICAL();  
    269                  
    270              }
    271              return 0 ;   
    272          }
