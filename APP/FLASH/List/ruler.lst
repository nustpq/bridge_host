   1360          /*
   1361          *********************************************************************************************************
   1362          *                                       Toggle_Mic()
   1363          *
   1364          * Description : Toggle specified mic's active state by sending command to related ruler and updating 
   1365          *               FPGA mic signal switch array. 
   1366          *               One mic One time.
   1367          * Argument(s) : pdata : pointer to TOGGLE_MIC structure data
   1368          * Return(s)   : NO_ERR :   execute successfully
   1369          *               others :   =error code .  
   1370          *
   1371          * Note(s)     : None.
   1372          *********************************************************************************************************
   1373          */
   1374          unsigned char Toggle_Mic(  TOGGLE_MIC *pdata )
   1375          {  
   1376              unsigned char  err ;
   1377              unsigned char  id;
   1378              unsigned int   mic_mask;  
   1379              unsigned int   fpga_mask;
   1380              
   1381          #if OS_CRITICAL_METHOD == 3u
   1382              OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
   1383          #endif 
   1384              fpga_mask = 0;
   1385              //check ruler connection state 
   1386              if( Global_Ruler_State[pdata->ruler_id] < RULER_STATE_CONFIGURED ) {      
   1387                  return RULER_STATE_ERR ;         
   1388              }  
   1389              APP_TRACE_INFO(("Toggle Ruler[%d]-Mic[%d] : %d  : ", pdata->ruler_id, pdata->mic_id, pdata->on_off )); 
   1390              OS_ENTER_CRITICAL(); 
   1391              mic_mask = Global_Mic_Mask[pdata->ruler_id];
   1392              OS_EXIT_CRITICAL();  
   1393              mic_mask &= ~( 1<<(pdata->mic_id));
   1394              mic_mask |=  (pdata->on_off&0x01)<<( pdata->mic_id);
   1395              err = Update_Mic_Mask( pdata->ruler_id, mic_mask );
   1396              APP_TRACE_INFO((" %s [0x%X]\r\n", err == OS_ERR_NONE ? "OK" : "FAIL" , err )); 
   1397              if( OS_ERR_NONE != err ) {        
   1398                  return err;    
   1399              }
   1400              OS_ENTER_CRITICAL(); 
   1401              Global_Mic_Mask[pdata->ruler_id] = mic_mask; 
   1402              //APP_TRACE_INFO(("Update Ruler[%d] Mic_Mask:  %d\r\n",pdata->ruler_id,Global_Mic_Mask[pdata->ruler_id]));   
   1403              if( mic_mask == 0 ) {      
   1404                  Global_Ruler_State[pdata->ruler_id] = RULER_STATE_CONFIGURED;         
   1405              } else {
   1406                  Global_Ruler_State[pdata->ruler_id] = RULER_STATE_SELECTED;  
   1407              }
   1408              OS_EXIT_CRITICAL();
   1409              if( RULER_TYPE_MASK( Global_Ruler_Type[pdata->ruler_id] ) == RULER_TYPE_RULER ) { //ruler
   1410                  for( id = 0; id < 4; id++ ) {
   1411                      fpga_mask += (Global_Mic_Mask[id]&0xFF) << (id<<3);
   1412                  }
   1413              } else { //handset
   1414                 fpga_mask = 0x3F << ((pdata->ruler_id)<<3);
   1415              }
   1416              Init_FPGA(fpga_mask);
   1417              return err;  
   1418          }
