   1477          /*
   1478          *********************************************************************************************************
   1479          *                                       Reset_Mic_Mask()
   1480          *
   1481          * Description : Reset all mics to deactived state on the specified rulers and update FPGA mic signal switch array.
   1482          * Argument(s) : pInt : pointer to a int data, the 4 bytes of wihch control 4 ruler's all mic need be 
   1483          *               reset to deactive state or not.
   1484          *                      1 - deactive all mics on this ruler
   1485          *                      0 - do nothing. ignore the reset operation
   1486          * Return(s)   : NO_ERR :   execute successfully
   1487          *               others :   =error code .  
   1488          *
   1489          * Note(s)     : None.
   1490          *********************************************************************************************************
   1491          */
   1492          unsigned char Reset_Mic_Mask(  unsigned int *pInt )
   1493          {  
   1494              unsigned char  err ;
   1495              unsigned char  id;
   1496              unsigned char  *pChar;  
   1497              unsigned int   fpga_mask;
   1498              
   1499              fpga_mask = 0;    
   1500              pChar     = (unsigned char *)pInt;
   1501              err       = 0;
   1502          
   1503              for( id = 0; id < 4; id++ ) {        
   1504                  if( *(pChar+id) == 0 ) { 
   1505                      continue;
   1506                  }
   1507                  if( Global_Ruler_State[id] < RULER_STATE_CONFIGURED ) { //why not RULER_STATE_SELECTED  ? Because UI need reset mic in any case
   1508                      continue;
   1509                  }
   1510                  Global_Ruler_State[id] = RULER_STATE_CONFIGURED ;
   1511                  err = Update_Mic_Mask( id, 0 );
   1512                  if( OS_ERR_NONE != err ) {        
   1513                      return err;    
   1514                  } 
   1515                  Global_Mic_Mask[id] = 0;  
   1516                  if( RULER_TYPE_MASK( Global_Ruler_Type[id] ) == RULER_TYPE_RULER ) {//ruler 
   1517                      fpga_mask += (Global_Mic_Mask[id]&0xFF) << (id<<3);
   1518                      
   1519                  } else {
   1520                      fpga_mask += 0x3F << (id<<3); //handset choose the lowest slot H01
   1521                  }        
   1522              }
   1523              
   1524              Init_FPGA(fpga_mask);    
   1525              return err;  
   1526          }
