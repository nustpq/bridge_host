   1487          /*
   1488          *********************************************************************************************************
   1489          *                                       Ruler_Port_LED_Service()
   1490          *
   1491          * Description : Control the ruler port identify LED state:  
   1492          *               turn on LED after ruler configured, blink LED during recording
   1493          * Argument(s) : None.
   1494          * Return(s)   : None.
   1495          * Note(s)     : None.
   1496          *********************************************************************************************************
   1497          */
   1498          void Ruler_Port_LED_Service( void )
   1499          {    
   1500              static unsigned int counter; 
   1501              static unsigned int counter_buz;    
   1502              unsigned char ruler_id;
   1503              unsigned char ruler_state;    
   1504              unsigned char LED_Freq;
   1505              unsigned char post_err_flag;
   1506          
   1507              LED_Freq      = 0x3F; 
   1508              post_err_flag = 0;
   1509              
   1510              for( ruler_id = 0 ; ruler_id < 4 ; ruler_id++ ) {
   1511                
   1512                  ruler_state = Global_Ruler_State[ruler_id];  
   1513                  if( Global_Bridge_POST != NO_ERR ) { //if POST err, start all LED 
   1514                      ruler_state = RULER_STATE_RUN ;
   1515                      post_err_flag = 1;
   1516                  }
   1517                  switch( ruler_state ) {
   1518                    
   1519                      case RULER_STATE_DETACHED :
   1520                      case RULER_STATE_ATTACHED :
   1521                          LED_Clear( LED_P0 + ruler_id );
   1522                      break;            
   1523                      case RULER_STATE_CONFIGURED :
   1524                      case RULER_STATE_SELECTED :  
   1525                          LED_Set( LED_P0 + ruler_id );
   1526                      break;            
   1527                      case RULER_STATE_RUN :
   1528                          if( (counter & LED_Freq) == 0 ) {
   1529                              LED_Toggle( LED_P0 + ruler_id );   
   1530                              if( post_err_flag== 1 && ruler_id == 0 && (counter_buz++ < 6 ) ) {
   1531                                  //Buzzer_Toggle(); //buzzer off id POST err 
   1532                                  Buzzer_OnOff( counter_buz&0x01 );   //fix long buz issue in some case                  
   1533                              }
   1534                          }
   1535                      
   1536                      default:              
   1537                      break;
   1538                  }
   1539            
   1540              }    
   1541              counter++;    
   1542          }
