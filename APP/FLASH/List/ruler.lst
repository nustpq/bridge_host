    464          *               others :   =error code . ruler connection error,           
    465          *
    466          * Note(s)     : None.
    467          *********************************************************************************************************
    468          */
    469          unsigned char Setup_Ruler( unsigned char ruler_slot_id ) //0 ~ 3
    470          {    
    471              unsigned char err ;
    472              EMB_BUF        *pEBuf_Data; 
    473              unsigned char buf[] = { RULER_CMD_SET_RULER, ruler_slot_id };
    474              
    475          #if OS_CRITICAL_METHOD == 3u
