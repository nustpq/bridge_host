    727                  //OS_ENTER_CRITICAL(); 
    728                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
    729                  //OS_EXIT_CRITICAL();  
    730                  UART1_Mixer( ruler_slot_id );
    731              }  
