    728              buf[0] =  RULER_CMD_WRITE_RULER_INFO;
    729              buf[1] =  EMB_DATA_FRAME;  
    730              buf[2] = (pEBuf_Cmd->length) & 0xFF;    
    731              buf[3] = ((pEBuf_Cmd->length)>>8) & 0xFF;     
    732          
    733              //check ruler connection state 
    734              if( Global_Ruler_State[ruler_slot_id] < RULER_STATE_CONFIGURED ) {      
    735                  return RULER_STATE_ERR ;         
    736              } 
    737              OSSemPend( UART_MUX_Sem_lock, 0, &err );    
    738              if( Global_Ruler_Index != ruler_slot_id ) {
