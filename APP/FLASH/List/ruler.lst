    748              pdata = pEBuf_Cmd->data;
    749              data_length = pEBuf_Cmd->length;
    750              while( data_length > 0 ){ 
    751                  temp = data_length > (NOAH_CMD_DATA_MLEN-1) ? (NOAH_CMD_DATA_MLEN-1) : data_length ;  
    752                  err = pcSendDateToBuf( EVENT_MsgQ_Noah2RulerUART, FRAM_TYPE_DATA, pdata, temp, 0, buf, 1 ) ; 
    753                  if( OS_ERR_NONE != err ) { break;}
    754                  OSTimeDly(50); //wait for ruler operation
    755                  data_length -= temp;
    756                  pdata += temp;
    757              }
    758              if( OS_ERR_NONE == err ) {
    759                  OSSemPend( Done_Sem_RulerUART, TIMEOUT_RULER_COM, &err );  
    760                  if( OS_ERR_TIMEOUT == err ) {
    761                      APP_TRACE_INFO(("Write_Ruler_Info[%d] timeout\r\n",ruler_slot_id));
