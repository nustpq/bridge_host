   1462              }
   1463              
   1464              if( pdata->spk == SET_VOLUME_MUTE ) {
   1465                  APP_TRACE_INFO(( "Mute SPK :  " ));
   1466              } else {
   1467                  APP_TRACE_INFO(( "SPK_Gain = -%d.%d dB :  ", pdata->spk/10, pdata->spk%10 )); 
