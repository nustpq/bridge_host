   1435          /*
   1436          *********************************************************************************************************
   1437          *                                       Reset_Mic_Mask()
   1438          *
   1439          * Description : Reset all mics to deactived state on the specified rulers and update FPGA mic signal switch array.
   1440          * Argument(s) : pInt : pointer to a int data, the 4 bytes of wihch control 4 ruler's all mic need be 
   1441          *               reset to deactive state or not.
   1442          *                      1 - deactive all mics on this ruler
   1443          *                      0 - do nothing. ignore the reset operation
   1444          * Return(s)   : NO_ERR :   execute successfully
   1445          *               others :   =error code .  
   1446          *
   1447          * Note(s)     : None.
   1448          *********************************************************************************************************
   1449          */
   1450          unsigned char Reset_Mic_Mask(  unsigned int *pInt )
   1451          {  
   1452              unsigned char  err ;
   1453              unsigned char  id;
   1454              unsigned char  *pChar;  
   1455              unsigned int   fpga_mask;
   1456              
   1457              fpga_mask = 0;    
   1458              pChar     = (unsigned char *)pInt;
   1459              err       = 0;
   1460          
   1461              for( id = 0; id < 4; id++ ) {        
   1462                  if( *(pChar+id) == 0 ) { 
   1463                      continue;
   1464                  }
   1465                  if( Global_Ruler_State[id] < RULER_STATE_CONFIGURED ) { //why not RULER_STATE_SELECTED  ? Because UI need reset mic in any case
   1466                      continue;
   1467                  }
   1468                  Global_Ruler_State[id] = RULER_STATE_CONFIGURED ;
   1469                  err = Update_Mic_Mask( id, 0 );
   1470                  if( OS_ERR_NONE != err ) {        
   1471                      return err;    
   1472                  } 
   1473                  Global_Mic_Mask[id] = 0;  
   1474                  if( RULER_TYPE_MASK( Global_Ruler_Type[id] ) == RULER_TYPE_RULER ) {//ruler 
   1475                      fpga_mask += (Global_Mic_Mask[id]&0xFF) << (id<<3);
   1476                      
   1477                  } else {
