    993          *********************************************************************************************************
    994          */
    995          unsigned char Get_Ruler_Version( unsigned char ruler_slot_id )
    996          {  
    997              unsigned char err ;
    998              unsigned char buf[] = { RULER_CMD_GET_RULER_VERSION };
    999              EMB_BUF      *pEBuf_Data;         
   1000                
   1001              pEBuf_Data  = &Emb_Buf_Data;  //Global var   
   1002              
   1003          #if OS_CRITICAL_METHOD == 3u
   1004              //OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
   1005          #endif 
   1006              //check ruler connection state 
   1007              if( Global_Ruler_State[ruler_slot_id] < RULER_STATE_ATTACHED ) {      
   1008                  return RULER_STATE_ERR ;         
   1009              }  
   1010              
   1011              OSSemPend( UART_MUX_Sem_lock, 0, &err ); 
   1012              if( Global_Ruler_Index != ruler_slot_id ) {
   1013                  Check_UART_Mixer_Ready();
   1014                  //OS_ENTER_CRITICAL(); 
   1015                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
   1016                  //OS_EXIT_CRITICAL();  
   1017                  UART1_Mixer( ruler_slot_id );
   1018              }
   1019              
   1020              err = pcSendDateToBuf( EVENT_MsgQ_Noah2RulerUART, FRAM_TYPE_DATA, buf, sizeof(buf), 0, NULL, 0 ) ; 
   1021              if( OS_ERR_NONE == err ) {
   1022                  OSSemPend( Done_Sem_RulerUART, TIMEOUT_RULER_COM, &err );  
   1023                  if( OS_ERR_TIMEOUT == err ) {
   1024                      APP_TRACE_INFO(("Get_Ruler_Version[%d] timeout\r\n",ruler_slot_id));
   1025                  } else {
   1026                      err = Ruler_CMD_Result;
   1027                      if(OS_ERR_NONE != err ){
   1028                          APP_TRACE_INFO(("Get_Ruler_Version[%d] err = %d\r\n",ruler_slot_id,err));
   1029                      }
   1030                  }
   1031                  if(err == OS_ERR_NONE ) {
   1032                      APP_TRACE_INFO(("Ruler[%d] FW Version: %s\r\n",ruler_slot_id, pEBuf_Data->data)); 
   1033                  }
   1034                  
   1035              } else {
   1036                  APP_TRACE_INFO(("Ruler[%d] pcSendDateToBuf failed: %d\r\n",ruler_slot_id,err));
   1037              }    
   1038              OSSemPost( UART_MUX_Sem_lock );    
   1039              return err ;        
   1040          }
