   1452              if( pdata->mic == SET_VOLUME_MUTE ) {
   1453                  APP_TRACE_INFO(( "Mute MIC :  " ));
   1454              } else {
   1455                  APP_TRACE_INFO(( "Mic_Gain = %d dB :  ", pdata->mic )); 
