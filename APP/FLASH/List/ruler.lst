    740                  //OS_ENTER_CRITICAL(); 
    741                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
    742                  //OS_EXIT_CRITICAL();  
    743                  UART1_Mixer( ruler_slot_id );
    744              }  
