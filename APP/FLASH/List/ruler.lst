    694          * Argument(s) : ruler_slot_id: 0~ 3.
    695          * Return(s)   : NO_ERR :   execute successfully
    696          *               others :   =error code .       
    697          *
    698          * Note(s)     : None.
    699          *********************************************************************************************************
    700          */
    701          unsigned char Write_Ruler_Info( unsigned char ruler_slot_id )
    702          {
    703              unsigned char   err;
    704              unsigned short  data_length;
    705              unsigned char   temp;
    706              unsigned char  *pdata;
    707              unsigned char   buf[4];  
    708              EMB_BUF        *pEBuf_Cmd;        
    709            
    710           #if OS_CRITICAL_METHOD == 3u
    711              //OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
