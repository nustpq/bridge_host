    141          /*
    142          *********************************************************************************************************
    143          *                                           Setup_Audio()
    144          *
    145          * Description : Send command to configure USB audio.
    146          * Argument(s) : pAudioCfg : pointer to AUDIO_CFG type data.
    147          * Return(s)   : NO_ERR :   execute successfully
    148          *               others :   refer to error code defines.           
    149          *
    150          * Note(s)     : None.
    151          *********************************************************************************************************
    152          */
    153          unsigned char Setup_Audio( AUDIO_CFG *pAudioCfg )
    154          {
    155              unsigned char err; 
    156              unsigned char mic_num; 
    157              unsigned char data  = 0xFF;
    158              unsigned char buf[] = { 
    159                  CMD_DATA_SYNC1, CMD_DATA_SYNC2, RULER_CMD_SET_AUDIO_CFG,\
    160                  pAudioCfg->type, pAudioCfg->channels,\
    161                 (pAudioCfg->sr)&0xFF, ((pAudioCfg->sr)>>8)&0xFF
    162              };
    163              
    164              //APP_TRACE_INFO(("Setup_Audio [%s]:[%d SR]:[%d CH]: %s\r\n",(pAudioCfg->type == 0) ? "REC " : "PLAY", pAudioCfg->sr, pAudioCfg->channels,((pAudioCfg->type == 0) && (pAudioCfg->lin_ch_mask == 0)) ? "LIN Disabled" : "LIN Enabled"));
    165              if( pAudioCfg->type == 0 ) {
    166                  APP_TRACE_INFO(("Setup_Audio [REC]:[%d SR]:[%d CH]: %s\r\n", pAudioCfg->sr, pAudioCfg->channels, pAudioCfg->lin_ch_mask == 0 ? "LIN Disabled" : "LIN Enabled" ));
    167              } else {
    168                  APP_TRACE_INFO(("Setup_Audio [PLAY]:[%d SR]:[%d CH]\r\n", pAudioCfg->sr, pAudioCfg->channels ));
    169              }
    170                   
    171              mic_num = Check_Actived_Mic_Number();
    172              if( mic_num > 6 ) {
    173                  APP_TRACE_INFO(("\r\nERROR: Check_Actived_Mic_Number = %d > 6\r\n",mic_num));
    174                  return AUD_CFG_MIC_NUM_MAX_ERR;//if report err, need UI support!  
    175              } 
    176              //check rec mic num    
    177              if( (pAudioCfg->type == 0) && ( mic_num != pAudioCfg->channels) ) {
    178                  APP_TRACE_INFO(("WARN:(Setup_Audio Rec)pAudioCfg->channels(%d) !=  Active MICs Num(%d)\r\n",pAudioCfg->channels,mic_num));
    179                  buf[4] = mic_num;
    180                  return AUD_CFG_MIC_NUM_DISMATCH_ERR;
    181              }
    182              //check channel num    
    183              if( (pAudioCfg->type == 1) && (pAudioCfg->channels == 0) ) {
    184                  APP_TRACE_INFO(("WARN:(Setup_Audio Play)pAudioCfg->channels =  0\r\n" ));        
    185                  //return AUD_CFG_PLAY_CH_ZERO_ERR;  UI not support
    186              }  
    187              if( (pAudioCfg->type == 0) && (pAudioCfg->channels == 0) && (pAudioCfg->lin_ch_mask == 0) ) {
    188                  APP_TRACE_INFO(("WARN:(Setup_Audio Rec)pAudioCfg->channels + ch_lin =  0\r\n" ));        
    189                  //return AUD_CFG_PLAY_CH_ZERO_ERR; UI not support
    190              }
    191              //check sample rate
    192              //No add here!
    193              //
    194              //
    195           #ifdef BOARD_TYPE_AB03    
    196              //check play ch num
    197              if(  (pAudioCfg->type == 1) && ( pAudioCfg->channels > 4 ) ) { //for AB03
    198                  APP_TRACE_INFO(("ERROR:(Setup_Audio Play)pAudioCfg->channels(=%d) > 4 NOT allowed for AB03\r\n",pAudioCfg->channels));
    199                  return AUD_CFG_PLAY_CH_ERR ;
    200              }
    201          #endif
    202              if ( (pAudioCfg->type == 0) && (pAudioCfg->lin_ch_mask != 0) ) {         
    203                   buf[4] += 2; //add 2 channel  
    204                   APP_TRACE_INFO(("Lin 2 channels added...%d\r\n",buf[4])); 
    205              }
    206              UART2_Mixer(3); 
    207              USART_SendBuf( AUDIO_UART, buf, sizeof(buf)) ; 
    208              err = USART_Read_Timeout( AUDIO_UART, &data, 1, TIMEOUT_AUDIO_COM);
    209              if( err != NO_ERR ) { 
    210                  APP_TRACE_INFO(("\r\nSetup_Audio ERROR: timeout\r\n")); 
    211                  return err;
    212              }
    213              if( data != NO_ERR ) {
    214                  APP_TRACE_INFO(("\r\nSetup_Audio ERROR: %d\r\n ",data)); 
    215                  return data; 
    216              }
    217              err = Init_CODEC( pAudioCfg->sr );
    218              if( err != NO_ERR ) {
    219                  APP_TRACE_INFO(("\r\nSetup_Audio Init_CODEC ERROR: %d\r\n",err)); 
    220              } 
    221          #ifdef BOARD_TYPE_AB03    
    222              err = Init_FM36_AB03( pAudioCfg->sr, mic_num, 1, 0 ); //Lin from SP1_RX, slot0~1
    223          #else
    224              err = ReInit_FM36( pAudioCfg->sr ); 
    225          #endif
    226              if( err != NO_ERR ) {
    227                  APP_TRACE_INFO(("\r\nSetup_Audio ReInit_FM36 ERROR: %d\r\n",err)); 
    228              }
    229              
    230          //    if ( pAudioCfg->lin_ch_mask != 0 ) {
    231          //        err = Set_AIC3204_DSP_Offset( mic_num );
    232          //        if( err != NO_ERR ) {
    233          //            APP_TRACE_INFO(("\r\nSetup_Audio Init AIC3204 ERROR: %d\r\n",err)); 
    234          //        }
    235          //    }
    236              
    237              return err ; 
    238          }
