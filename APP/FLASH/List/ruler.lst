   1208          /*
   1209          *********************************************************************************************************
   1210          *                                       Update_Ruler_FW()
   1211          *
   1212          * Description :  Write firmware to specified ruler's MCU flash
   1213          *               
   1214          * Argument(s) :  ruler_slot_id :  0~ 3.     
   1215          *
   1216          * Return(s)   : NO_ERR :   execute successfully
   1217          *               others :   =error code .  
   1218          *
   1219          * Note(s)     : Do not care if ruler is attached or not.Because host can not detect ruler if FW was crashed.
   1220          *********************************************************************************************************
   1221          */
   1222          unsigned char Update_Ruler_FW( unsigned char ruler_slot_id )
   1223          {
   1224              unsigned char err;
   1225              unsigned int  flash_addr; 
   1226              FLASH_INFO   *pFlash_Info;
   1227              unsigned char Buf[9];
   1228              unsigned char i;
   1229              
   1230              err = NO_ERR;
   1231              flash_addr  = FLASH_ADDR_FW_BIN;
   1232              pFlash_Info = (FLASH_INFO *)FLASH_ADDR_FW_STATE ;
   1233                  
   1234              if( pFlash_Info->f_w_state != FW_DOWNLAD_STATE_FINISHED ) {
   1235                  APP_TRACE_INFO(("ERROR: FW bin file missed!\r\n"));        
   1236                  return FW_BIN_STATE_ERR;
   1237              }
   1238              
   1239              APP_TRACE_INFO(("Start updating ruler[%d] firmware to \"%s\" version ...\r\n",ruler_slot_id,pFlash_Info->bin_name)); 
   1240              memset(Buf,'d',sizeof(Buf)); //send 'd' to start download  
   1241              Ruler_Power_Switch(0);   //power off ruler  
   1242              OSTimeDly(200);   
   1243              for( i = 0; i < 4; i++ ) {
   1244                  Global_Ruler_State[i] = RULER_STATE_DETACHED ;
   1245              }
   1246              UART_Init(RULER_UART,  NULL,  115200 );   //Init Ruler to inquire mode
   1247              Port_Detect_Enable(0); //disable ruler detect
   1248              
   1249              OSSemPend( UART_MUX_Sem_lock, 0, &err ); 
   1250              if( Global_Ruler_Index != ruler_slot_id ) {
   1251                  Check_UART_Mixer_Ready();
   1252                  //OS_ENTER_CRITICAL(); 
   1253                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
   1254                  //OS_EXIT_CRITICAL();  
   1255                  UART1_Mixer( ruler_slot_id );
   1256              }
   1257              
   1258              Ruler_Power_Switch(1);   //power on ruler
   1259              OSTimeDly(200);
   1260              err = USART_SendBuf( RULER_UART, Buf,  sizeof(Buf));
   1261              if( OS_ERR_NONE == err ) {     
   1262                  OSTimeDly(800);
   1263                  err = USART_Read_Timeout( RULER_UART, Buf, 3, 5000 );
   1264                  if( OS_ERR_NONE == err && ( Buf[0] == 'c' || Buf[0] == 'C' )) {
   1265                      Global_Ruler_State[ruler_slot_id] = RULER_STATE_RUN ;
   1266                      err = Xmodem_Transmit( (unsigned char *)flash_addr, pFlash_Info->bin_size );
   1267                      Global_Ruler_State[ruler_slot_id] = RULER_STATE_DETACHED ;            
   1268                  }         
   1269              }
   1270              if( OS_ERR_NONE != err ) {
   1271                  APP_TRACE_INFO(("\r\nFailed to init ruler bootloader. Err Code = [0x%X]\r\n", err));        
   1272              } else {
   1273                  APP_TRACE_INFO(("\r\nUpdate ruler[%d] firmware successfully!\r\n", ruler_slot_id));   
   1274              }
   1275              Port_Detect_Enable(1); //enable ruler detect
   1276              UART_Init(RULER_UART,  ISR_Ruler_UART,  115200 );  //Init Ruler back to interuption mode
   1277              Ruler_Power_Switch(0);   //power off ruler  
   1278              OSTimeDly(500);    
   1279              Ruler_Power_Switch(1);   //power on ruler
   1280              OSSemPost( UART_MUX_Sem_lock ); 
   1281              return err ;    
   1282              
   1283          }
