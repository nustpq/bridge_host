    653          * Return(s)   : NO_ERR :   execute successfully
    654          *               others :   =error code .       
    655          *
    656          * Note(s)     : None.
    657          *********************************************************************************************************
    658          */
    659          unsigned char Write_Ruler_Info( unsigned char ruler_slot_id )
    660          {
    661              unsigned char   err;
    662              unsigned short  data_length;
    663              unsigned char   temp;
    664              unsigned char  *pdata;
    665              unsigned char   buf[4];  
    666              EMB_BUF        *pEBuf_Cmd;        
