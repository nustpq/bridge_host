   1318          /*
   1319          *********************************************************************************************************
   1320          *                                       Toggle_Mic()
   1321          *
   1322          * Description : Toggle specified mic's active state by sending command to related ruler and updating 
   1323          *               FPGA mic signal switch array. 
   1324          *               One mic One time.
   1325          * Argument(s) : pdata : pointer to TOGGLE_MIC structure data
   1326          * Return(s)   : NO_ERR :   execute successfully
   1327          *               others :   =error code .  
   1328          *
   1329          * Note(s)     : None.
   1330          *********************************************************************************************************
   1331          */
   1332          unsigned char Toggle_Mic(  TOGGLE_MIC *pdata )
   1333          {  
   1334              unsigned char  err ;
   1335              unsigned char  id;
   1336              unsigned int   mic_mask;  
   1337              unsigned int   fpga_mask;
   1338              
   1339          #if OS_CRITICAL_METHOD == 3u
   1340              OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
   1341          #endif 
   1342              fpga_mask = 0;
   1343              //check ruler connection state 
   1344              if( Global_Ruler_State[pdata->ruler_id] < RULER_STATE_CONFIGURED ) {      
   1345                  return RULER_STATE_ERR ;         
   1346              }  
   1347              APP_TRACE_INFO(("Toggle Ruler[%d]-Mic[%d] : %d  : ", pdata->ruler_id, pdata->mic_id, pdata->on_off )); 
   1348              OS_ENTER_CRITICAL(); 
   1349              mic_mask = Global_Mic_Mask[pdata->ruler_id];
   1350              OS_EXIT_CRITICAL();  
   1351              mic_mask &= ~( 1<<(pdata->mic_id));
   1352              mic_mask |=  (pdata->on_off&0x01)<<( pdata->mic_id);
   1353              err = Update_Mic_Mask( pdata->ruler_id, mic_mask );
   1354              APP_TRACE_INFO((" %s [0x%X]\r\n", err == OS_ERR_NONE ? "OK" : "FAIL" , err )); 
   1355              if( OS_ERR_NONE != err ) {        
   1356                  return err;    
   1357              }
   1358              OS_ENTER_CRITICAL(); 
   1359              Global_Mic_Mask[pdata->ruler_id] = mic_mask; 
   1360              //APP_TRACE_INFO(("Update Ruler[%d] Mic_Mask:  %d\r\n",pdata->ruler_id,Global_Mic_Mask[pdata->ruler_id]));   
   1361              if( mic_mask == 0 ) {      
   1362                  Global_Ruler_State[pdata->ruler_id] = RULER_STATE_CONFIGURED;         
   1363              } else {
   1364                  Global_Ruler_State[pdata->ruler_id] = RULER_STATE_SELECTED;  
   1365              }
   1366              OS_EXIT_CRITICAL();
   1367              if( RULER_TYPE_MASK( Global_Ruler_Type[pdata->ruler_id] ) == RULER_TYPE_RULER ) { //ruler
   1368                  for( id = 0; id < 4; id++ ) {
   1369                      fpga_mask += (Global_Mic_Mask[id]&0xFF) << (id<<3);
   1370                  }
   1371              } else { //handset
   1372                 fpga_mask = 0x3F << ((pdata->ruler_id)<<3);
   1373              }
   1374              Init_FPGA(fpga_mask);
   1375              return err;  
   1376          }
