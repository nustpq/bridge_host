   1397          /*
   1398          *********************************************************************************************************
   1399          *                                       Toggle_Mic()
   1400          *
   1401          * Description : Toggle specified mic's active state by sending command to related ruler and updating 
   1402          *               FPGA mic signal switch array. 
   1403          *               One mic One time.
   1404          * Argument(s) : pdata : pointer to TOGGLE_MIC structure data
   1405          * Return(s)   : NO_ERR :   execute successfully
   1406          *               others :   =error code .  
   1407          *
   1408          * Note(s)     : None.
   1409          *********************************************************************************************************
   1410          */
   1411          unsigned char Toggle_Mic(  TOGGLE_MIC *pdata )
   1412          {  
   1413              unsigned char  err ;
   1414              unsigned char  id;
   1415              unsigned int   mic_mask;  
   1416              unsigned int   fpga_mask;
   1417              unsigned char  temp;
   1418              
   1419          #if OS_CRITICAL_METHOD == 3u
   1420              OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
   1421          #endif 
   1422              fpga_mask = 0;
   1423              //check ruler connection state 
   1424              if( Global_Ruler_State[pdata->ruler_id] < RULER_STATE_CONFIGURED ) {      
   1425                  return RULER_STATE_ERR ;         
   1426              }  
   1427              APP_TRACE_INFO(("Toggle Ruler[%d]-Mic[%d] : %d  : ", pdata->ruler_id, pdata->mic_id, pdata->on_off )); 
   1428              OS_ENTER_CRITICAL(); 
   1429              mic_mask = Global_Mic_Mask[pdata->ruler_id];
   1430              OS_EXIT_CRITICAL();  
   1431              mic_mask &= ~( 1<<(pdata->mic_id));
   1432              mic_mask |=  (pdata->on_off&0x01)<<( pdata->mic_id);
   1433              err = Update_Mic_Mask( pdata->ruler_id, mic_mask );
   1434              APP_TRACE_INFO((" %s [0x%X]\r\n", err == OS_ERR_NONE ? "OK" : "FAIL" , err )); 
   1435              if( OS_ERR_NONE != err ) {        
   1436                  return err;    
   1437              }
   1438              OS_ENTER_CRITICAL(); 
   1439              Global_Mic_Mask[pdata->ruler_id] = mic_mask; 
   1440              //APP_TRACE_INFO(("Update Ruler[%d] Mic_Mask:  %d\r\n",pdata->ruler_id,Global_Mic_Mask[pdata->ruler_id]));   
   1441              if( mic_mask == 0 ) {      
   1442                  Global_Ruler_State[pdata->ruler_id] = RULER_STATE_CONFIGURED;         
   1443              } else {
   1444                  Global_Ruler_State[pdata->ruler_id] = RULER_STATE_SELECTED;  
   1445              }
   1446              OS_EXIT_CRITICAL();
   1447              if( RULER_TYPE_MASK( Global_Ruler_Type[pdata->ruler_id] ) == RULER_TYPE_RULER ) { //ruler
   1448                  for( id = 0; id < 4; id++ ) {            
   1449                      if( Global_Ruler_Type[pdata->ruler_id] == RULER_TYPE_W01 ) {
   1450                          temp = Revert_Bit_Order( Global_Mic_Mask[id]&0xFF, 8 );
   1451                      } else {
   1452                          temp = Global_Mic_Mask[id]&0xFF;
   1453                      }
   1454                      fpga_mask += temp << (id<<3);
   1455                  }
   1456              } else { //handset
   1457                 fpga_mask = 0x3F << ((pdata->ruler_id)<<3);
   1458              }
   1459              Init_FPGA(fpga_mask);
   1460              return err;  
   1461          }
