   1124          /*
   1125          *********************************************************************************************************
   1126          *                                       Save_Ruler_FW()
   1127          *
   1128          * Description : Save ruler FW bin file to flash
   1129          *               
   1130          * Argument(s) :  cmd  :  1~ 3.
   1131          *               *pBin : pointer to bin file data packge to be wriiten to flash
   1132          *               *pStr : pointer to file name string
   1133          *                size : bin package file size 
   1134          *
   1135          * Return(s)   : NO_ERR :   execute successfully
   1136          *               others :   =error code .  
   1137          *
   1138          * Note(s)     : None.
   1139          *********************************************************************************************************
   1140          */
   1141          unsigned char Save_Ruler_FW( unsigned int cmd, unsigned char *pBin, unsigned char *pStr, unsigned int size )
   1142          {  
   1143              unsigned char err; 
   1144              static unsigned int flash_addr = FLASH_ADDR_FW_BIN;
   1145           
   1146              FLASH_INFO    flash_info;
   1147              
   1148              err = NO_ERR;
   1149              Read_Flash_State(&flash_info);
   1150               
   1151              switch( cmd ) {
   1152                  case FW_DOWNLAD_CMD_START :
   1153                      APP_TRACE_INFO(("Start loading ruler bin file to AB01 flash ... \r\n"));
   1154                      flash_addr = FLASH_ADDR_FW_BIN;                
   1155                      flash_info.f_w_state = FW_DOWNLAD_STATE_UNFINISHED ;
   1156                      flash_info.bin_size  = 0;
   1157                  break;   
   1158                  case FW_DOWNLAD_CMD_DOING :
   1159                      APP_TRACE_INFO(("> ")); 
   1160                      if( flash_info.f_w_state != FW_DOWNLAD_STATE_UNFINISHED ) {
   1161                          APP_TRACE_INFO(("ERROR: flash state not match!\r\n"));
   1162                          err  =  FW_BIN_STATE_0_ERR;                
   1163                      } 
   1164                  break;
   1165                  case FW_DOWNLAD_CMD_DONE :
   1166                      APP_TRACE_INFO((">\r\n")); 
   1167                      if( flash_info.f_w_state != FW_DOWNLAD_STATE_UNFINISHED ) {
   1168                          APP_TRACE_INFO(("ERROR: flash state not match!\r\n"));
   1169                          err  =  FW_BIN_STATE_1_ERR;
   1170                          break;
   1171                      }
   1172                      flash_info.f_w_state = FW_DOWNLAD_STATE_FINISHED ;
   1173                      flash_info.f_w_counter++;            
   1174                   break;
   1175                   
   1176                   default:
   1177                      APP_TRACE_INFO(("ERROR:  Save ruler FW bad cmd!\r\n"));
   1178                      err = FW_BIN_SAVE_CMD_ERR;    
   1179                   break;
   1180                  
   1181              }
   1182              if( err != NO_ERR ) {
   1183                  return err;
   1184              }    
   1185              Buzzer_OnOff(1);               
   1186              LED_Toggle(LED_DS2);    
   1187              err = FLASHD_Write_Safe( flash_addr, pBin, size ); 
   1188              Buzzer_OnOff(0); 
   1189              if(err != NO_ERR ) {                     
   1190                  APP_TRACE_INFO(("ERROR: Write MCU flash failed!\r\n"));
   1191                  return err;
   1192              }
   1193              flash_addr += size;
   1194              flash_info.bin_size   = flash_addr - FLASH_ADDR_FW_BIN ;
   1195              strcpy(flash_info.bin_name, (char const*)pStr);  
   1196              if( cmd != FW_DOWNLAD_CMD_DOING ) {        
   1197                  err = Write_Flash_State( &flash_info ); 
   1198                  if( err == NO_ERR && cmd == FW_DOWNLAD_CMD_DONE ) { 
   1199                        APP_TRACE_INFO(("Bin file[%d Btyes] saved successfully!\r\n",flash_info.bin_size));     
   1200                  }   
   1201              } 
   1202              return err;  
   1203              
   1204          }
