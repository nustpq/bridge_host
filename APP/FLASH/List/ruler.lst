   1166          /*
   1167          *********************************************************************************************************
   1168          *                                       Save_Ruler_FW()
   1169          *
   1170          * Description : Save ruler FW bin file to flash
   1171          *               
   1172          * Argument(s) :  cmd  :  1~ 3.
   1173          *               *pBin : pointer to bin file data packge to be wriiten to flash
   1174          *               *pStr : pointer to file name string
   1175          *                size : bin package file size 
   1176          *
   1177          * Return(s)   : NO_ERR :   execute successfully
   1178          *               others :   =error code .  
   1179          *
   1180          * Note(s)     : None.
   1181          *********************************************************************************************************
   1182          */
   1183          unsigned char Save_Ruler_FW( unsigned int cmd, unsigned char *pBin, unsigned char *pStr, unsigned int size )
   1184          {  
   1185              unsigned char err; 
   1186              static unsigned int flash_addr = FLASH_ADDR_FW_BIN;
   1187           
   1188              FLASH_INFO    flash_info;
   1189              
   1190              err = NO_ERR;
   1191              Read_Flash_State(&flash_info);
   1192               
   1193              switch( cmd ) {
   1194                  case FW_DOWNLAD_CMD_START :
   1195                      APP_TRACE_INFO(("Start loading ruler bin file to AB01 flash ... \r\n"));
   1196                      flash_addr = FLASH_ADDR_FW_BIN;                
   1197                      flash_info.f_w_state = FW_DOWNLAD_STATE_UNFINISHED ;
   1198                      flash_info.bin_size  = 0;
   1199                  break;   
   1200                  case FW_DOWNLAD_CMD_DOING :
   1201                      APP_TRACE_INFO(("> ")); 
   1202                      if( flash_info.f_w_state != FW_DOWNLAD_STATE_UNFINISHED ) {
   1203                          APP_TRACE_INFO(("ERROR: flash state not match!\r\n"));
   1204                          err  =  FW_BIN_STATE_0_ERR;                
   1205                      } 
   1206                  break;
   1207                  case FW_DOWNLAD_CMD_DONE :
   1208                      APP_TRACE_INFO((">\r\n")); 
   1209                      if( flash_info.f_w_state != FW_DOWNLAD_STATE_UNFINISHED ) {
   1210                          APP_TRACE_INFO(("ERROR: flash state not match!\r\n"));
   1211                          err  =  FW_BIN_STATE_1_ERR;
   1212                          break;
   1213                      }
   1214                      flash_info.f_w_state = FW_DOWNLAD_STATE_FINISHED ;
   1215                      flash_info.f_w_counter++;            
   1216                   break;
   1217                   
   1218                   default:
   1219                      APP_TRACE_INFO(("ERROR:  Save ruler FW bad cmd!\r\n"));
   1220                      err = FW_BIN_SAVE_CMD_ERR;    
   1221                   break;
   1222                  
   1223              }
   1224              if( err != NO_ERR ) {
   1225                  return err;
   1226              }    
   1227              Buzzer_OnOff(1);               
   1228              LED_Toggle(LED_DS2);    
   1229              err = FLASHD_Write_Safe( flash_addr, pBin, size ); 
   1230              Buzzer_OnOff(0); 
   1231              if(err != NO_ERR ) {                     
   1232                  APP_TRACE_INFO(("ERROR: Write MCU flash failed!\r\n"));
   1233                  return err;
   1234              }
   1235              flash_addr += size;
   1236              flash_info.bin_size   = flash_addr - FLASH_ADDR_FW_BIN ;
   1237              strcpy(flash_info.bin_name, (char const*)pStr);  
   1238              if( cmd != FW_DOWNLAD_CMD_DOING ) {        
   1239                  err = Write_Flash_State( &flash_info ); 
   1240                  if( err == NO_ERR && cmd == FW_DOWNLAD_CMD_DONE ) { 
   1241                        APP_TRACE_INFO(("Bin file[%d Btyes] saved successfully!\r\n",flash_info.bin_size));     
   1242                  }   
   1243              } 
   1244              return err;  
   1245              
   1246          }
