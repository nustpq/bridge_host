   1013                  err = pcSendDateToBuf( EVENT_MsgQ_Noah2RulerUART, FRAM_TYPE_DATA, buf, sizeof(buf), 0, NULL, 0 ) ; 
   1014                  if( OS_ERR_NONE == err ) {
   1015                      OSSemPend( Done_Sem_RulerUART, TIMEOUT_RULER_COM, &err );  
   1016                      if( OS_ERR_TIMEOUT == err ) {
   1017                          APP_TRACE_INFO(("Ruler[%d]_Active_Control timeout\r\n",ruler_id));
   1018                      } else {
   1019                          err = Ruler_CMD_Result; //exe result from GACK
   1020                          if(OS_ERR_NONE != err ){
   1021                              APP_TRACE_INFO(("Ruler[%d]_Active_Control err = %d\r\n",ruler_id,err));
   1022                          }
   1023                      }
   1024                      
   1025                  } else {
   1026                      APP_TRACE_INFO(("Ruler[%d] pcSendDateToBuf failed: %d\r\n",ruler_id,err));
   1027                  }    
   1028                  OSSemPost( UART_MUX_Sem_lock );
   1029                  if( err != NO_ERR ) {
   1030                      break;
   1031                  }
   1032              }        
   1033              return err ;    
   1034          }
