   1522          *                                       Reset_Mic_Mask()
   1523          *
   1524          * Description : Reset all mics to deactived state on the specified rulers and update FPGA mic signal switch array.
   1525          * Argument(s) : pInt : pointer to a int data, the 4 bytes of wihch control 4 ruler's all mic need be 
   1526          *               reset to deactive state or not.
   1527          *                      1 - deactive all mics on this ruler
   1528          *                      0 - do nothing. ignore the reset operation
   1529          * Return(s)   : NO_ERR :   execute successfully
   1530          *               others :   =error code .  
   1531          *
   1532          * Note(s)     : None.
   1533          *********************************************************************************************************
   1534          */
   1535          unsigned char Reset_Mic_Mask(  unsigned int *pInt )
   1536          {  
   1537              unsigned char  err ;
   1538              unsigned char  id;
   1539              unsigned char  *pChar;  
   1540              unsigned int   fpga_mask;
   1541              
   1542              fpga_mask = 0;    
   1543              pChar     = (unsigned char *)pInt;
   1544              err       = 0;
   1545          
   1546              for( id = 0; id < 4; id++ ) {        
   1547                  if( *(pChar+id) == 0 ) { 
   1548                      continue;
   1549                  }
   1550                  if( Global_Ruler_State[id] < RULER_STATE_CONFIGURED ) { //why not RULER_STATE_SELECTED  ? Because UI need reset mic in any case
   1551                      continue;
   1552                  }
   1553                  Global_Ruler_State[id] = RULER_STATE_CONFIGURED ;
   1554                  err = Update_Mic_Mask( id, 0 );
   1555                  if( OS_ERR_NONE != err ) {        
   1556                      return err;    
   1557                  } 
   1558                  Global_Mic_Mask[id] = 0;  
   1559                  if( RULER_TYPE_MASK( Global_Ruler_Type[id] ) == RULER_TYPE_RULER ) {//ruler 
   1560                      fpga_mask += (Global_Mic_Mask[id]&0xFF) << (id<<3);
   1561                      
   1562                  } else {
   1563                      fpga_mask += 0x3F << (id<<3); //handset choose the lowest slot H01
   1564                  }        
   1565              }
   1566              
   1567              Init_FPGA(fpga_mask);    
   1568              return err;  
   1569          }
