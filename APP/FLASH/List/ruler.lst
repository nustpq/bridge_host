    714              pEBuf_Cmd  = &Emb_Buf_Cmd; //Golbal var
    715              buf[0] =  RULER_CMD_WRITE_RULER_INFO;
    716              buf[1] =  EMB_DATA_FRAME;  
    717              buf[2] = (pEBuf_Cmd->length) & 0xFF;    
    718              buf[3] = ((pEBuf_Cmd->length)>>8) & 0xFF;     
    719          
    720              //check ruler connection state 
    721              if( Global_Ruler_State[ruler_slot_id] < RULER_STATE_CONFIGURED ) {      
    722                  return RULER_STATE_ERR ;         
    723              } 
    724              OSSemPend( UART_MUX_Sem_lock, 0, &err );    
