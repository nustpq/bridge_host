    735              pdata = pEBuf_Cmd->data;
    736              data_length = pEBuf_Cmd->length;
    737              while( data_length > 0 ){ 
    738                  temp = data_length > (NOAH_CMD_DATA_MLEN-1) ? (NOAH_CMD_DATA_MLEN-1) : data_length ;  
    739                  err = pcSendDateToBuf( EVENT_MsgQ_Noah2RulerUART, FRAM_TYPE_DATA, pdata, temp, 0, buf, 1 ) ; 
    740                  if( OS_ERR_NONE != err ) { break;}
    741                  OSTimeDly(50); //wait for ruler operation
    742                  data_length -= temp;
    743                  pdata += temp;
    744              }
    745              if( OS_ERR_NONE == err ) {
    746                  OSSemPend( Done_Sem_RulerUART, TIMEOUT_RULER_COM, &err );  
    747                  if( OS_ERR_TIMEOUT == err ) {
    748                      APP_TRACE_INFO(("Write_Ruler_Info[%d] timeout\r\n",ruler_slot_id));
    749                  } else {
    750                      err = Ruler_CMD_Result; //exe result from GACK
    751                      if(OS_ERR_NONE != err ){
    752                          APP_TRACE_INFO(("Write_Ruler_Info[%d] err = %d\r\n",ruler_slot_id,err));
    753                      }
    754                  }
    755                  
    756              } else {
    757                  APP_TRACE_INFO(("Ruler[%d] pcSendDateToBuf failed: %d\r\n",ruler_slot_id,err));
    758              }
    759              OSSemPost( UART_MUX_Sem_lock );
    760              
    761              return err ;
    762              
    763          }
