    360          /*
    361          *********************************************************************************************************
    362          *                                       Init_Ruler()
    363          *
    364          * Description : Communicate with ruler to check connected or not
    365          *    
    366          * Argument(s) : ruler_slot_id: 0~ 3.
    367          * Return(s)   : NO_ERR :   ruler connected
    368          *               others :   =error code . ruler connection error,           
    369          *
    370          * Note(s)     : None.
    371          *********************************************************************************************************
    372          */
    373          unsigned char Init_Ruler( unsigned char ruler_slot_id ) //0 ~ 3
    374          {
    375              unsigned char err ;
    376          
    377          #if OS_CRITICAL_METHOD == 3u
    378              //OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
    379          #endif 
    380                
    381              //check ruler connection state 
    382              if( Global_Ruler_State[ruler_slot_id] < RULER_STATE_ATTACHED ) {      
    383                  return RULER_STATE_ERR ;         
    384              } 
    385              OSSemPend( UART_MUX_Sem_lock, 0, &err );
    386              if( Global_Ruler_Index != ruler_slot_id ) {
    387                  Check_UART_Mixer_Ready();
    388                  //OS_ENTER_CRITICAL(); 
    389                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
    390                  //OS_EXIT_CRITICAL();  
    391                  UART1_Mixer( ruler_slot_id );
    392              } 
    393              
    394              err = pcSendDateToBuf( EVENT_MsgQ_Noah2RulerUART, FRAM_TYPE_EST, NULL, 0, 0, NULL, 0 ) ; 
    395              if( OS_ERR_NONE == err ) {
    396                  OSSemPend( Done_Sem_RulerUART, TIMEOUT_RULER_COM, &err );  
    397                  if( OS_ERR_TIMEOUT == err ) {
    398                      APP_TRACE_INFO(("Init ruler[%d] timeout!\r\n",ruler_slot_id));            
    399                  } else {
    400                      err = Ruler_CMD_Result; //exe result from GACK
    401                      if(OS_ERR_NONE != err ){
    402                          APP_TRACE_INFO(("Init_Ruler[%d] err = %d\r\n",ruler_slot_id,err));
    403                      }
    404                  }
    405                  
    406              } else {
    407                  APP_TRACE_INFO(("Ruler[%d] pcSendDateToBuf failed: %d\r\n",ruler_slot_id,err));        
    408              }    
    409              OSSemPost( UART_MUX_Sem_lock );    
    410              return err ;    
    411          }
