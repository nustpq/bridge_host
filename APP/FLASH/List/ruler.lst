    708                      err = Ruler_CMD_Result; //exe result from GACK
    709                      if(OS_ERR_NONE != err ){
    710                          APP_TRACE_INFO(("Write_Ruler_Info[%d] err = %d\r\n",ruler_slot_id,err));
    711                      }
    712                  }
    713                  
    714              } else {
    715                  APP_TRACE_INFO(("Ruler[%d] pcSendDateToBuf failed: %d\r\n",ruler_slot_id,err));
    716              }
    717              OSSemPost( UART_MUX_Sem_lock );
    718              
    719              return err ;
    720              
    721          }
