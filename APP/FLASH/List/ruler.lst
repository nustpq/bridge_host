   1179          /*
   1180          *********************************************************************************************************
   1181          *                                       Save_Ruler_FW()
   1182          *
   1183          * Description : Save ruler FW bin file to flash
   1184          *               
   1185          * Argument(s) :  cmd  :  1~ 3.
   1186          *               *pBin : pointer to bin file data packge to be wriiten to flash
   1187          *               *pStr : pointer to file name string
   1188          *                size : bin package file size 
   1189          *
   1190          * Return(s)   : NO_ERR :   execute successfully
   1191          *               others :   =error code .  
   1192          *
   1193          * Note(s)     : None.
   1194          *********************************************************************************************************
   1195          */
   1196          unsigned char Save_Ruler_FW( unsigned int cmd, unsigned char *pBin, unsigned char *pStr, unsigned int size )
   1197          {  
   1198              unsigned char err; 
   1199              static unsigned int flash_addr = FLASH_ADDR_FW_BIN;
   1200           
   1201              FLASH_INFO    flash_info;
   1202              
   1203              err = NO_ERR;
   1204              Read_Flash_State(&flash_info);
   1205               
   1206              switch( cmd ) {
   1207                  case FW_DOWNLAD_CMD_START :
   1208                      APP_TRACE_INFO(("Start loading ruler bin file to AB01 flash ... \r\n"));
   1209                      flash_addr = FLASH_ADDR_FW_BIN;                
   1210                      flash_info.f_w_state = FW_DOWNLAD_STATE_UNFINISHED ;
   1211                      flash_info.bin_size  = 0;
   1212                  break;   
   1213                  case FW_DOWNLAD_CMD_DOING :
   1214                      APP_TRACE_INFO(("> ")); 
   1215                      if( flash_info.f_w_state != FW_DOWNLAD_STATE_UNFINISHED ) {
   1216                          APP_TRACE_INFO(("ERROR: flash state not match!\r\n"));
   1217                          err  =  FW_BIN_STATE_0_ERR;                
   1218                      } 
   1219                  break;
   1220                  case FW_DOWNLAD_CMD_DONE :
   1221                      APP_TRACE_INFO((">\r\n")); 
   1222                      if( flash_info.f_w_state != FW_DOWNLAD_STATE_UNFINISHED ) {
   1223                          APP_TRACE_INFO(("ERROR: flash state not match!\r\n"));
   1224                          err  =  FW_BIN_STATE_1_ERR;
   1225                          break;
   1226                      }
   1227                      flash_info.f_w_state = FW_DOWNLAD_STATE_FINISHED ;
   1228                      flash_info.f_w_counter++;            
   1229                   break;
   1230                   
   1231                   default:
   1232                      APP_TRACE_INFO(("ERROR:  Save ruler FW bad cmd!\r\n"));
   1233                      err = FW_BIN_SAVE_CMD_ERR;    
   1234                   break;
   1235                  
   1236              }
   1237              if( err != NO_ERR ) {
   1238                  return err;
   1239              }    
   1240              Buzzer_OnOff(1);               
   1241              LED_Toggle(LED_DS2);    
   1242              err = FLASHD_Write_Safe( flash_addr, pBin, size ); 
   1243              Buzzer_OnOff(0); 
   1244              if(err != NO_ERR ) {                     
   1245                  APP_TRACE_INFO(("ERROR: Write MCU flash failed!\r\n"));
   1246                  return err;
   1247              }
   1248              flash_addr += size;
   1249              flash_info.bin_size   = flash_addr - FLASH_ADDR_FW_BIN ;
   1250              strcpy(flash_info.bin_name, (char const*)pStr);  
   1251              if( cmd != FW_DOWNLAD_CMD_DOING ) {        
   1252                  err = Write_Flash_State( &flash_info ); 
   1253                  if( err == NO_ERR && cmd == FW_DOWNLAD_CMD_DONE ) { 
   1254                        APP_TRACE_INFO(("Bin file[%d Btyes] saved successfully!\r\n",flash_info.bin_size));     
   1255                  }   
   1256              } 
   1257              return err;  
   1258              
   1259          }
