    763                      err = Ruler_CMD_Result; //exe result from GACK
    764                      if(OS_ERR_NONE != err ){
    765                          APP_TRACE_INFO(("Write_Ruler_Info[%d] err = %d\r\n",ruler_slot_id,err));
    766                      }
    767                  }
    768                  
    769              } else {
    770                  APP_TRACE_INFO(("Ruler[%d] pcSendDateToBuf failed: %d\r\n",ruler_slot_id,err));
    771              }
    772              OSSemPost( UART_MUX_Sem_lock );
    773              
    774              return err ;
    775              
    776          }
