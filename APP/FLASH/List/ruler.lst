    685                  //OS_ENTER_CRITICAL(); 
    686                  Global_Ruler_Index = ruler_slot_id ; //for ruler status switch in TX/RX/Noah 
    687                  //OS_EXIT_CRITICAL();  
    688                  UART1_Mixer( ruler_slot_id );
    689              }  
