   1086          /*
   1087          *********************************************************************************************************
   1088          *                                       FLASHD_Write_Safe()
   1089          *
   1090          * Description : Add code area protection for FLASHD_Write()
   1091          *               Writes a data buffer in the internal flash. This function works in polling
   1092          *               mode, and thus only returns when the data has been effectively written.    
   1093          * Argument(s) :  address  Write address.
   1094          *                pBuffer  Data buffer.
   1095          *                size     Size of data buffer in bytes.
   1096          * Return(s)   : NO_ERR :   execute successfully
   1097          *               others :   =error code .  
   1098          *
   1099          * Note(s)     : None.
   1100          *********************************************************************************************************
   1101          */
   1102          unsigned char FLASHD_Write_Safe( unsigned int address, const void *pBuffer,  unsigned int size)
   1103          {
   1104              unsigned char err;
   1105              if( address < AT91C_IFLASH + AT91C_IFLASH_CODE_SIZE ) {
   1106                  APP_TRACE_INFO(("ERROR: this operation wanna flush code area!\r\n"));  
   1107                  return FW_BIN_SAVE_ADDR_ERR;
   1108              }
   1109              err = FLASHD_Write(  address, pBuffer, size );
   1110              return err;  
   1111              
   1112          }
