   1542          /*
   1543          *********************************************************************************************************
   1544          *                                       Ruler_Port_LED_Service()
   1545          *
   1546          * Description : Control the ruler port identify LED state:  
   1547          *               turn on LED after ruler configured, blink LED during recording
   1548          * Argument(s) : None.
   1549          * Return(s)   : None.
   1550          * Note(s)     : None.
   1551          *********************************************************************************************************
   1552          */
   1553          void Ruler_Port_LED_Service( void )
   1554          {    
   1555              static unsigned int counter; 
   1556              static unsigned int counter_buz;    
   1557              unsigned char ruler_id;
   1558              unsigned char ruler_state;    
   1559              unsigned char LED_Freq;
   1560              unsigned char post_err_flag;
   1561          
   1562              LED_Freq      = 0x3F; 
   1563              post_err_flag = 0;
   1564              
   1565              for( ruler_id = 0 ; ruler_id < 4 ; ruler_id++ ) {
   1566                
   1567                  ruler_state = Global_Ruler_State[ruler_id];  
   1568                  if( Global_Bridge_POST != NO_ERR ) { //if POST err, start all LED 
   1569                      ruler_state = RULER_STATE_RUN ;
   1570                      post_err_flag = 1;
   1571                  }
   1572                  switch( ruler_state ) {
   1573                    
   1574                      case RULER_STATE_DETACHED :
   1575                      case RULER_STATE_ATTACHED :
   1576                          LED_Clear( LED_P0 + ruler_id );
   1577                      break;            
   1578                      case RULER_STATE_CONFIGURED :
   1579                      case RULER_STATE_SELECTED :  
   1580                          LED_Set( LED_P0 + ruler_id );
   1581                      break;            
   1582                      case RULER_STATE_RUN :
   1583                          if( (counter & LED_Freq) == 0 ) {
   1584                              LED_Toggle( LED_P0 + ruler_id );   
   1585                              if( post_err_flag== 1 && ruler_id == 0 && (counter_buz++ < 6 ) ) {
   1586                                  //Buzzer_Toggle(); //buzzer off id POST err 
   1587                                  Buzzer_OnOff( counter_buz&0x01 );   //fix long buz issue in some case                  
   1588                              }
   1589                          }
   1590                      
   1591                      default:              
   1592                      break;
   1593                  }
   1594            
   1595              }    
   1596              counter++;    
   1597          }
