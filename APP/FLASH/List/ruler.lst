    241          /*
    242          *********************************************************************************************************
    243          *                                           Start_Audio()
    244          *
    245          * Description : Send command to start USB audio play/record.
    246          * Argument(s) : cmd_type : record£¨== 1£©/play£¨== 2£©/record & play £¨== 3£©
    247          * Return(s)   : NO_ERR :   execute successfully
    248          *               others :   refer to error code defines.           
    249          *
    250          * Note(s)     : None.
    251          *********************************************************************************************************
    252          */
    253          unsigned char Start_Audio( unsigned char cmd_type )
    254          {   
    255              unsigned char err   = 0xFF;  
    256              unsigned char data  = 0xFF; 
    257              unsigned char ruler_id;    
    258              unsigned char buf[] = { CMD_DATA_SYNC1, CMD_DATA_SYNC2, RULER_CMD_START_AUDIO, cmd_type&0x03 }; 
    259              
    260          #if OS_CRITICAL_METHOD == 3u
    261              OS_CPU_SR  cpu_sr = 0u;                                 /* Storage for CPU status register         */
    262          #endif 
    263              APP_TRACE_INFO(("Start_Audio : type = [%d]\r\n", cmd_type));
    264              UART2_Mixer(3); 
    265              USART_SendBuf( AUDIO_UART, buf,  sizeof(buf)) ;    
    266              err = USART_Read_Timeout( AUDIO_UART, &data, 1, TIMEOUT_AUDIO_COM);  
    267              if( err != NO_ERR ) { 
    268                  APP_TRACE_INFO(("\r\nStart_Audio ERROR: Timeout : %d\r\n",err)); 
    269                  return err;
    270              }
    271              if( data != NO_ERR ) {
    272                  APP_TRACE_INFO(("\r\nStart_Audio ERROR: Data : %d\r\n ",data)); 
    273                  return data; 
    274              } else {
    275                  OS_ENTER_CRITICAL(); 
    276                  for( ruler_id = 0 ; ruler_id < 4 ; ruler_id++ ) {       
    277                      if( Global_Ruler_State[ruler_id] ==  RULER_STATE_SELECTED ) {//given: if mic selected, then ruler used
    278                          Global_Ruler_State[ruler_id] = RULER_STATE_RUN;                 
    279                      }      
    280                  }
    281                  OS_EXIT_CRITICAL();  
    282                  
    283              }
    284              return 0 ;   
    285          }
