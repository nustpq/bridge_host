   1490          /*
   1491          *********************************************************************************************************
   1492          *                                       Reset_Mic_Mask()
   1493          *
   1494          * Description : Reset all mics to deactived state on the specified rulers and update FPGA mic signal switch array.
   1495          * Argument(s) : pInt : pointer to a int data, the 4 bytes of wihch control 4 ruler's all mic need be 
   1496          *               reset to deactive state or not.
   1497          *                      1 - deactive all mics on this ruler
   1498          *                      0 - do nothing. ignore the reset operation
   1499          * Return(s)   : NO_ERR :   execute successfully
   1500          *               others :   =error code .  
   1501          *
   1502          * Note(s)     : None.
   1503          *********************************************************************************************************
   1504          */
   1505          unsigned char Reset_Mic_Mask(  unsigned int *pInt )
   1506          {  
   1507              unsigned char  err ;
   1508              unsigned char  id;
   1509              unsigned char  *pChar;  
   1510              unsigned int   fpga_mask;
   1511              
   1512              fpga_mask = 0;    
   1513              pChar     = (unsigned char *)pInt;
   1514              err       = 0;
   1515          
   1516              for( id = 0; id < 4; id++ ) {        
   1517                  if( *(pChar+id) == 0 ) { 
   1518                      continue;
   1519                  }
   1520                  if( Global_Ruler_State[id] < RULER_STATE_CONFIGURED ) { //why not RULER_STATE_SELECTED  ? Because UI need reset mic in any case
   1521                      continue;
   1522                  }
   1523                  Global_Ruler_State[id] = RULER_STATE_CONFIGURED ;
   1524                  err = Update_Mic_Mask( id, 0 );
   1525                  if( OS_ERR_NONE != err ) {        
   1526                      return err;    
   1527                  } 
   1528                  Global_Mic_Mask[id] = 0;  
   1529                  if( RULER_TYPE_MASK( Global_Ruler_Type[id] ) == RULER_TYPE_RULER ) {//ruler 
   1530                      fpga_mask += (Global_Mic_Mask[id]&0xFF) << (id<<3);
   1531                      
   1532                  } else {
   1533                      fpga_mask += 0x3F << (id<<3); //handset choose the lowest slot H01
   1534                  }        
   1535              }
   1536              
   1537              Init_FPGA(fpga_mask);    
   1538              return err;  
   1539          }
