     41          /// Returns the minimum value between two integers.
     42          /// \param a  First integer to compare.
     43          /// \param b  Second integer to compare.
     44          //------------------------------------------------------------------------------
     45          unsigned int min(unsigned int a, unsigned int b)
     46          { 
     47              if (a < b) {
     48          
     49                  return a;
     50              }
     51              else {
     52          
     53                  return b;
     54              }
     55          }
