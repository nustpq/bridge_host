     57          //------------------------------------------------------------------------------
     58          /// Returns the absolute value of an integer.
     59          /// \param value  Integer value.
     60          //------------------------------------------------------------------------------
     61          // Do not call this function "abs", problem with gcc !
     62          unsigned int absv(signed int value)
     63          {
     64              if (value < 0) {
     65          
     66                  return -value;
     67              }
     68              else {
     69          
     70                  return value;
     71              }
     72          }
     73          
     74          //------------------------------------------------------------------------------
     75          /// Computes and returns x power of y.
     76          /// \param x  Value.
     77          /// \param y  Power.
     78          //------------------------------------------------------------------------------
     79          unsigned int power(unsigned int x, unsigned int y)
     80          {
     81              unsigned int result = 1;
     82              
     83              while (y > 0) {
     84          
     85                  result *= x;
     86                  y--;
     87              } 
     88              return result;
     89          }
